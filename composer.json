{
	"name": "natanfelles/email",
	"type": "library",
	"description": "The Framework Email Library",
	"keywords": [
		"email"
	],
	"homepage": "https://the-framework.gitlab.io/libraries/email.html",
	"license": "MIT",
	"authors": [
		{
			"name": "Natan Felles",
			"email": "natanfelles@gmail.com"
		}
	],
	"require": {
		"php": ">=7.3",
		"ext-imap": "*",
		"ext-openssl": "*"
	},
	"require-dev": {
		"ext-xdebug": "*",
		"localheinz/composer-normalize": "^1.3",
		"natanfelles/coding-standard": "^1.0",
		"phpmd/phpmd": "^2.7",
		"phpstan/phpstan": "^0.11",
		"phpunit/phpunit": "^8.3"
	},
	"autoload": {
		"psr-4": {
			"Framework\\Email\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\Email\\": "tests/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"scripts": {
		"post-install-cmd": [
			".hooks/init"
		],
		"post-update-cmd": [
			"composer dump-autoload"
		]
	},
	"support": {
		"email": "natanfelles@gmail.com",
		"issues": "https://gitlab.com/the-framework/libraries/email/issues",
		"wiki": "https://gitlab.com/the-framework/libraries/email/wikis/home",
		"source": "https://gitlab.com/the-framework/libraries/email",
		"docs": "https://the-framework.gitlab.io/libraries/email/docs/"
	}
}
